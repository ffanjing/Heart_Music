<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>RecommendServlet</servlet-name>
        <servlet-class>RecommendServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RecommendServlet</servlet-name>
        <url-pattern>/RecommendServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MayLikeServlet</servlet-name>
        <servlet-class>MayLikeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MayLikeServlet</servlet-name>
        <url-pattern>/MayLikeServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetAllSongsServlet</servlet-name>
        <servlet-class>GetAllSongsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllSongsServlet</servlet-name>
        <url-pattern>/GetAllSongsServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>PlaySongServlet</servlet-name>
        <servlet-class>PlaySongServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PlaySongServlet</servlet-name>
        <url-pattern>/PlaySongServlet</url-pattern>
    </servlet-mapping>

</web-app>
